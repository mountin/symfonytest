{{ form_errors(form) }}
<form name="book" method="post">
    {{ form_row(form._token) }}
    {{ form_row(form.title) }}
    {{ form_row(form.description) }}
    {{ form_row(form.value) }}
    {{ form_row(form.author) }}
    {{ form_row(form.genre) }}
    <button class="btn">{{ button_label|default('Save') }}</button>
</form>